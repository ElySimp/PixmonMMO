    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.48",
    "multer": "^2.0.0",
    "mysql2": "^3.14.1",
    "node-cron": "^4.1.0",
    "node-fetch": "^3.3.2",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
